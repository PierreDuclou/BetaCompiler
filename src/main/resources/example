factorielle(int n) -> int {
    if (n <= 1) {
        return 1;
    } else {
        return factorielle(n - 1);
    }
}

main() -> void {
    b = 11 + (12 * 5);

    if (b > 0) {
        b = 0;
    }

    while (b < 10) {
        if (b == 0) {
            if (factorielle(4) == 24) {
                b = b + 2;
            }
        }

        b = b + 1;
    }

    int a = factorielle(b);
    int c = 45;
}

int b;
