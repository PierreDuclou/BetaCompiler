int b;

factorielle(int n) -> int {
    if (n <= 1) {
        return 1;
    }

    return n * factorielle(n-1);
}

main() -> void {
    b = 11 + (12 * 5);

    if (b > 0) {
        b = 0;
    }

    while (b < 10) {
        b = 123;
    }

    int a = factorielle(b);
    int c = 45;
    int d = c;
    a = d;
    write(a);
    int k = read();
}